generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dream {
  id        String   @id @default(cuid())
  content   String
  type      String   @default("dream")
  date      String
  tags      String   @default("[]")
  analysis  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([date])
}

